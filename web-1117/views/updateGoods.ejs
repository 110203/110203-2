      <section class="content-section" data-background="#fffbf7">
        <div class="container">
          <div class="row justify-content-center">
            <div class="col-md-12" style="margin-top: 2%;">

              <div class="tab-panels" style="max-width: 65em; margin: auto;">
                <section id="addGood" class="tab-panel">
                  <form enctype="multipart/form-data" action="/updateG" method="POST">
                    <div class="row mb-3">
                      <label for="inputEmail3" class="col-sm-2 col-form-label">物品編號</label>
                      
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" style="width: 100%!important;"name='gNo' value="<%= items.gNo %>"  readonly>
                      </div>
                    </div>

                    <div class="row mb-3">
                      <label for="inputEmail3" class="col-sm-2 col-form-label">展覽編號</label>
                      
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" style="width: 100%!important;" name='eNo' value="<%= items.eNo %>" readonly>
                      </div>
                    </div>

                    <div class="row mb-3">
                      <label for="exampleInputEmail1" class="col-sm-2 col-form-label">商品名稱</label>
                      
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" style="width: 100%!important;" name='gName'value="<%= items.gName %>" required>
                      </div>
                    </div>

                    <div class="row mb-3">
                      <label for="exampleInputEmail1" class="col-sm-2 col-form-label">商品簡介</label>
                      
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" style="width: 100%!important;" name='introdution'value="<%= items.introdution %>"required>
                      </div>
                    </div>

                    <div class="row mb-3">
                      <label for="exampleInputEmail1" class="col-sm-2 col-form-label">商品圖片</label>
                      
                      <div class="col-sm-10">
                        <div style="margin-top: 1.2%;">
                          <img src="/file/<%= items.gImage2D %>" class="img-preview">
                        </div>
                        <br>                      
                          <div class="image-upload-wrap">
                            <input  type='file' class="file-upload-input" onchange="readURL(this);"  name='gImage2D' accept="image/*" />
                            <div class="drag-text">
                              <h3>拖曳或選擇所需上傳的圖</h3>
                            </div>
                          </div>
                          <div class="file-upload-content">
                            <img class="file-upload-image" src="/file/<%= items.gImage2D %>" alt="your image" />
                            <div class="image-title-wrap">
                              <button type="button" onclick="removeUpload()" class="remove-image">移除 <span class="image-title">已上傳的圖片</span></button>
                            </div>
                          </div>  
                      </div>
                    </div>                      

                    <div class="row mb-3">
                      <label for="exampleInputEmail1" class="col-sm-2 col-form-label">商品數量</label>
                      
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" style="width: 100%!important;" name='gAmount'value="<%= items.gAmount %>"required>
                      </div>
                    </div>

                    <div class="row mb-3">
                      <label for="exampleInputEmail1" class="col-sm-2 col-form-label">商品價錢</label>
                      
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" style="width: 100%!important;" name='price'value="<%= items.price %>"required>
                      </div>
                    </div>

                    <div class="row mb-3">
                      <label for="exampleInputEmail1" class="col-sm-2 col-form-label">商品狀態</label>
                      
                      <div class="col-sm-10" style="margin-top: 0.5%;">
                        <p>
                          <input type="radio" id="exist" name="gDelete" value="0" checked>
                          <label for="exist">存在</label>
                        </p>
                        <p>
                          <input type="radio" id="gone" name="gDelete" value="1">
                          <label for="gone">刪除</label>
                        </p>
                      </div>
                    </div>

                    <br><br>
                    <div class="col-12 text-center">
                      <input class="btn btn-block mb-4" type="submit" value="送出">
                    </div>
                  </form>
